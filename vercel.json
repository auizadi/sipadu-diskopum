{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.4" }
    },
    "routes": [
        { "src": "/(css|js|build)/(.*)", "dest": "/public/$1/$2" },
        {
            "src": "/(.*\\.(png|jpg|jpeg|gif|webp|svg|ico|txt|pdf))",
            "dest": "/public/$1"
        },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "public": true,
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://sipadu-diskopum.vercel.app",

        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "cookie",
        "LOG_CHANNEL": "stderr",

        "VIEW_COMPILED_PATH": "/tmp",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",

        "DB_CONNECTION": "mysql",
        "DB_HOST": "rynbiv.h.filess.io",
        "DB_PORT": "3306",
        "DB_DATABASE": "sipaduDB_unionworry",
        "DB_USERNAME": "sipaduDB_unionworry"
    }
}
