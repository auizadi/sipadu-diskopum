{
    "version": 2,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.4" }
    },
    "routes": [
        { "src": "/(css|js|build)/(.*)", "dest": "/public/$1/$2" },
        {
            "src": "/(.*\\.(png|jpg|jpeg|gif|webp|svg|ico))",
            "dest": "/public/$1"
        },
        { "src": "/(.*)", "dest": "/public/index.php" }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://yourprojectname.vercel.app",
        "APP_KEY": "base64:hm2iP8weodd6S2QmPdXh092dyJ/8QyAKMkt5sntAhrE=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}
